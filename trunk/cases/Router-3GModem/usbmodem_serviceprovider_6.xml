<?xml version="1.0" encoding="UTF-8"?>
<testcase>
    <brief>SIM card from other providers</brief>
    <priority>High</priority>
    <automated>no</automated>
    <description>
        
		Pre-Condition:

			1) The 3g service is activated and available
			2) WAN through 3g modem with original SIM card works well

		Test Procedure:

			1) Unplug the origianl SIM card and plug a new SIM card from other providers
			2) WAN through usbmodem
			3) Ping an internet address
    </description>
    <numofap>1</numofap>
    <numofsta>1</numofsta>
    <tbtype>winlin</tbtype>
    <property name="" value=""/>
    <stage>
                <step no="1">
            <desc>
            1) The 3g service is activated and available
            </desc>
            <log-file></log-file>
            <telnet target="ap1"></telnet>
            <ssh target="sta1"></ssh>
            <setvar name=""></setvar>
            <script></script>
            <log-match></log-match>
        </step>
        <step no="2">
            <desc>
            2) WAN through 3g modem with original SIM card works well
            </desc>
            <log-file></log-file>
            <telnet target="ap1"></telnet>
            <ssh target="sta1"></ssh>
            <setvar name=""></setvar>
            <script></script>
            <log-match></log-match>
        </step>
        <step no="3">
            <desc>
            1) Unplug the origianl SIM card and plug a new SIM card from other providers
            </desc>
            <log-file></log-file>
            <telnet target="ap1"></telnet>
            <ssh target="sta1"></ssh>
            <setvar name=""></setvar>
            <script></script>
            <log-match></log-match>
        </step>
        <step no="4">
            <desc>
            2) WAN through usbmodem
            </desc>
            <log-file></log-file>
            <telnet target="ap1"></telnet>
            <ssh target="sta1"></ssh>
            <setvar name=""></setvar>
            <script></script>
            <log-match></log-match>
        </step>
        <step no="5">
            <desc>
            3) Ping an internet address
            </desc>
            <log-file></log-file>
            <telnet target="ap1"></telnet>
            <ssh target="sta1"></ssh>
            <setvar name=""></setvar>
            <script></script>
            <log-match></log-match>
        </step>

    </stage>
    <failed-process>
        <step no="1..n">
            .. process
	</step>
    </failed-process>
</testcase>
