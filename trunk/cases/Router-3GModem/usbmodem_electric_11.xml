<?xml version="1.0" encoding="UTF-8"?>
<testcase>
    <brief>Status of LED with/ without track invalid ip after capwap is enabled, usbmodem is prim  WAN</brief>
    <priority>Low</priority>
    <automated>no</automated>
    <description>
        
		Pre-Condition:

			1) WAN is available by usbmodem
			2) Set the mode of USB modem as primary-wan
			3) capwap is enabled initially
			4) Unplug eth0

		Test Procedure:

			1) Startup BR with track valid ip
			2) Check the state of WAN and LED
			3) Track invalid ip
			track QS-IP-Track-Router ip 1.1.1.1
			4) Repeat step 2)
			5) Stop track invalid ip
			no track QS-IP-Track-Router ip 1.1.1.1
			6) Repeat step 2)
    </description>
    <numofap>1</numofap>
    <numofsta>1</numofsta>
    <tbtype>winlin</tbtype>
    <property name="" value=""/>
    <stage>
                <step no="1">
            <desc>
            1) WAN is available by usbmodem
            </desc>
            <log-file></log-file>
            <telnet target="ap1"></telnet>
            <ssh target="sta1"></ssh>
            <setvar name=""></setvar>
            <script></script>
            <log-match></log-match>
        </step>
        <step no="2">
            <desc>
            2) Set the mode of USB modem as primary-wan
            </desc>
            <log-file></log-file>
            <telnet target="ap1"></telnet>
            <ssh target="sta1"></ssh>
            <setvar name=""></setvar>
            <script></script>
            <log-match></log-match>
        </step>
        <step no="3">
            <desc>
            3) capwap is enabled initially
            </desc>
            <log-file></log-file>
            <telnet target="ap1"></telnet>
            <ssh target="sta1"></ssh>
            <setvar name=""></setvar>
            <script></script>
            <log-match></log-match>
        </step>
        <step no="4">
            <desc>
            4) Unplug eth0
            </desc>
            <log-file></log-file>
            <telnet target="ap1"></telnet>
            <ssh target="sta1"></ssh>
            <setvar name=""></setvar>
            <script></script>
            <log-match></log-match>
        </step>
        <step no="5">
            <desc>
            1) Startup BR with track valid ip
            </desc>
            <log-file></log-file>
            <telnet target="ap1"></telnet>
            <ssh target="sta1"></ssh>
            <setvar name=""></setvar>
            <script></script>
            <log-match></log-match>
        </step>
        <step no="6">
            <desc>
            2) Check the state of WAN and LED
            </desc>
            <log-file></log-file>
            <telnet target="ap1"></telnet>
            <ssh target="sta1"></ssh>
            <setvar name=""></setvar>
            <script></script>
            <log-match></log-match>
        </step>
        <step no="7">
            <desc>
            3) Track invalid ip
				track QS-IP-Track-Router ip 1.1.1.1
            </desc>
            <log-file></log-file>
            <telnet target="ap1"></telnet>
            <ssh target="sta1"></ssh>
            <setvar name=""></setvar>
            <script></script>
            <log-match></log-match>
        </step>
        <step no="8">
            <desc>
            4) Repeat step 2)
            </desc>
            <log-file></log-file>
            <telnet target="ap1"></telnet>
            <ssh target="sta1"></ssh>
            <setvar name=""></setvar>
            <script></script>
            <log-match></log-match>
        </step>
        <step no="9">
            <desc>
            5) Stop track invalid ip
				no track QS-IP-Track-Router ip 1.1.1.1
            </desc>
            <log-file></log-file>
