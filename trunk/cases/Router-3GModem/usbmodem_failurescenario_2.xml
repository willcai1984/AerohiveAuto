			</telnet>
			<log-match>-c -i "from ${pub.dns1.ip}" -i "ttl="</log-match>
		</step> 
 
		<step no="5">
			<desc>
				shutdown interface eth0
			</desc>
			<console target="br1.br">
				-v "interface eth0 shutdown"
			</console>
		</step>
