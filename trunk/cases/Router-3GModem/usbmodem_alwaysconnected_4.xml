			</telnet>
			<log-match bin="${bin.dir}/searchblocks.py">-ps p "${des.addr}" p "1" p "eth0" -f ${log.dir}/${log.file}</log-match>
			<log-match bin="${bin.dir}/searchblocks.py">-ps p "${des.addr}" p "600" p "ppp0" -f ${log.dir}/${log.file}</log-match>
