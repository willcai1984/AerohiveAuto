<?xml version="1.0" encoding="UTF-8"?>
<testcase>
    <brief>Report modem errors to HiveManager</brief>
    <priority>High</priority>
    <automated>no</automated>
    <description>
        
		Pre-Condition:

			1) Config usbmodem mode as primary-wan
			2) The 3g service is activated and available
			3) Configured the dialup username/ passwd/ number if the modem if necessary

		Test Procedure:

			1) Run some illegal action to trigger modem error
			2) Check the error msg of modem on HM
    </description>
    <numofap>1</numofap>
    <numofsta>1</numofsta>
    <tbtype>winlin</tbtype>
    <property name="" value=""/>
    <stage>
                <step no="1">
            <desc>
            1) Config usbmodem mode as primary-wan
            </desc>
            <log-file></log-file>
            <telnet target="ap1"></telnet>
            <ssh target="sta1"></ssh>
            <setvar name=""></setvar>
            <script></script>
            <log-match></log-match>
        </step>
        <step no="2">
            <desc>
            2) The 3g service is activated and available
            </desc>
            <log-file></log-file>
            <telnet target="ap1"></telnet>
            <ssh target="sta1"></ssh>
            <setvar name=""></setvar>
            <script></script>
            <log-match></log-match>
        </step>
        <step no="3">
            <desc>
            3) Configured the dialup username/ passwd/ number if the modem if necessary
            </desc>
            <log-file></log-file>
            <telnet target="ap1"></telnet>
            <ssh target="sta1"></ssh>
            <setvar name=""></setvar>
            <script></script>
            <log-match></log-match>
        </step>
        <step no="4">
            <desc>
            1) Run some illegal action to trigger modem error
            </desc>
            <log-file></log-file>
            <telnet target="ap1"></telnet>
            <ssh target="sta1"></ssh>
            <setvar name=""></setvar>
            <script></script>
            <log-match></log-match>
        </step>
        <step no="5">
            <desc>
            2) Check the error msg of modem on HM
            </desc>
            <log-file></log-file>
            <telnet target="ap1"></telnet>
            <ssh target="sta1"></ssh>
            <setvar name=""></setvar>
            <script></script>
            <log-match></log-match>
        </step>

    </stage>
    <failed-process>
        <step no="1..n">
            .. process
	</step>
    </failed-process>
</testcase>
