			-v "ssid ${ssid.name}_6"
			-v "ssid ${ssid.name}_7"
		    -v "ssid ${ssid.name}_8"
			-v "in w0 ssid ${ssid.name}"
			-v "in w0 ssid ${ssid.name}_2"
			-v "in w0 ssid ${ssid.name}_3"
			-v "in w0 ssid ${ssid.name}_4"
			-v "in w0 ssid ${ssid.name}_5"
			-v "in w0 ssid ${ssid.name}_6"
			-v "in w0 ssid ${ssid.name}_7"
