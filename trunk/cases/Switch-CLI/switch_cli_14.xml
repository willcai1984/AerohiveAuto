<?xml version="1.0" encoding="UTF-8"?>
<testcase>
	<brief> Bean CLI check </brief>
	<priority>High</priority>
	<automated>yes</automated>
	<numofsw>1</numofsw>
	<tbtype>winlin</tbtype>

	<description>
    CLI: Check CLI commands
	</description>
	<comment>
	</comment>
	<property name="sw1.user" value="admin" />
	<property name="sw1.passwd" value="aerohive" />
	<property name="colour" value="Amber" />
	<stage>    	
		<step no="1.0">
            <desc> Show CLI commands </desc>
            <log-file>capture_cmds.log</log-file>
            <ssh target="sw1" timeout="120">
            -v "show cmds"
            </ssh>
        </step>
	</stage>
	<failed-process>
		<stage>
		    <step no="2.0">
                 <desc>
                 recovery env
                 </desc>
			     <log-file>reboot.log</log-file>
                 <console target="sw1" slow="1">
                -v "reboot"
                </console>
                <no-error-check />
		    </step>
		</stage>
	</failed-process>
</testcase>