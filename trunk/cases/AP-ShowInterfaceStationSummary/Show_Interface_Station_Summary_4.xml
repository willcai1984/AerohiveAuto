<?xml version="1.0" encoding="UTF-8"?>
<testcase>
    <brief>Show_Interface_Station_Summary_4.</brief>
    <priority>Accept</priority>
    <automated>yes</automated>
    <description>
		Pre-Condition:
		Test Procedure:
    </description>
    <numofap>1</numofap>
    <numofsta>0</numofsta>
    <tbtype>linux</tbtype>
    
    <stage>
		<step no="1">
			<desc>init config</desc>
			<log-file>step1.log</log-file>
			<console target="ap1">
				-v "_test wifi-summary crc_error_ratio 20 tx_error_ratio 1 channel_util 1"
			</console>
		</step>
	
		<step no="2">
			<desc>check wifi summary state</desc>
			<log-file>step2.log</log-file>
			<console target="ap1">
				-v 'show int wifi0 | include "Summary state"'
			</console>
			<no-error-check />
			<log-match>-c -i "Summary state=High collision;" </log-match>	
		</step>	
	</stage>
