<?xml version="1.0" encoding="UTF-8"?>
<testcase>
    <brief>Verify the function of modify allowed usage time of the account on GM.</brief>
    <priority>High</priority>
    <automated>no</automated>
    <description>
        
		Pre-Condition:

			

		Test Procedure:

			1.	Create an PPSK account on GM, set the allowed usage time as 1 hour.
			2.	Connect to the SSID using the PPSK.
			3.	Before the expiration time is due, add an hour to the account.
			4.	Check the connection of the client an hour after the client's 
			connecting. [result 1]
			5.	Check the connection of the client an hour later.
    </description>
    <numofap>1</numofap>
    <numofsta>1</numofsta>
    <tbtype>winlin</tbtype>
    <property name="" value=""/>
    <stage>
                <step no="1">
            <desc>
            1.	Create an PPSK account on GM, set the allowed usage time as 1 hour.
            </desc>
            <log-file></log-file>
            <telnet target="ap1"></telnet>
            <ssh target="sta1"></ssh>
            <setvar name=""></setvar>
            <script></script>
            <log-match></log-match>
        </step>
        <step no="2">
            <desc>
            2.	Connect to the SSID using the PPSK.
            </desc>
            <log-file></log-file>
            <telnet target="ap1"></telnet>
            <ssh target="sta1"></ssh>
            <setvar name=""></setvar>
            <script></script>
            <log-match></log-match>
        </step>
        <step no="3">
            <desc>
            3.	Before the expiration time is due, add an hour to the account.
            </desc>
            <log-file></log-file>
            <telnet target="ap1"></telnet>
            <ssh target="sta1"></ssh>
            <setvar name=""></setvar>
            <script></script>
            <log-match></log-match>
        </step>
        <step no="4">
            <desc>
            4.	Check the connection of the client an hour after the client's 
				connecting. [result 1]
            </desc>
            <log-file></log-file>
            <telnet target="ap1"></telnet>
            <ssh target="sta1"></ssh>
            <setvar name=""></setvar>
            <script></script>
            <log-match></log-match>
        </step>
        <step no="5">
            <desc>
            5.	Check the connection of the client an hour later.
            </desc>
            <log-file></log-file>
            <telnet target="ap1"></telnet>
            <ssh target="sta1"></ssh>
            <setvar name=""></setvar>
            <script></script>
            <log-match></log-match>
        </step>

    </stage>
    <failed-process>
        <step no="1..n">
            .. process
	</step>
    </failed-process>
</testcase>
