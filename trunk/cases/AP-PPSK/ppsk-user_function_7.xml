<?xml version="1.0" encoding="UTF-8"?>
<testcase>
    <brief>Verify that when 9999 user accounts are configured for a user group, the PMK  matching time cost is reasonable.</brief>
    <priority>High</priority>
    <automated>no</automated>
    <description>
        
		Pre-Condition:

			

		Test Procedure:

			1.	Configure a user group, 'ug1'
			2.	Configure 9999 user accounts for the user group
			3.	Try to connect to the SSID using the last user accounts.
			4.	Repeat the step 3 under different conditions:
			When cpu load is high or not. [result 1]
    </description>
    <numofap>1</numofap>
    <numofsta>1</numofsta>
    <tbtype>winlin</tbtype>
    <property name="" value=""/>
    <stage>
                <step no="1">
            <desc>
            1.	Configure a user group, 'ug1'
            </desc>
            <log-file></log-file>
            <telnet target="ap1"></telnet>
            <ssh target="sta1"></ssh>
            <setvar name=""></setvar>
            <script></script>
            <log-match></log-match>
        </step>
        <step no="2">
            <desc>
            2.	Configure 9999 user accounts for the user group
            </desc>
            <log-file></log-file>
            <telnet target="ap1"></telnet>
            <ssh target="sta1"></ssh>
            <setvar name=""></setvar>
            <script></script>
            <log-match></log-match>
        </step>
        <step no="3">
            <desc>
            3.	Try to connect to the SSID using the last user accounts.
            </desc>
            <log-file></log-file>
            <telnet target="ap1"></telnet>
            <ssh target="sta1"></ssh>
            <setvar name=""></setvar>
            <script></script>
            <log-match></log-match>
        </step>
        <step no="4">
            <desc>
            4.	Repeat the step 3 under different conditions:
				When cpu load is high or not. [result 1]
            </desc>
            <log-file></log-file>
            <telnet target="ap1"></telnet>
            <ssh target="sta1"></ssh>
            <setvar name=""></setvar>
            <script></script>
            <log-match></log-match>
        </step>

    </stage>
    <failed-process>
        <step no="1..n">
            .. process
	</step>
    </failed-process>
</testcase>
