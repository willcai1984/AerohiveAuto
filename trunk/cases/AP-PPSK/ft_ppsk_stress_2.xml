<?xml version="1.0" encoding="UTF-8"?>
<testcase>
    <brief>GTK/GMK/PTK rekey when max clients are connected for a long time.</brief>
    <priority>High</priority>
    <automated>no</automated>
    <description>
        
		Pre-Condition:

			

		Test Procedure:

			1.	Configure a PPSK SSID, bind to wifi0
			2.	Configure a user group, set it to the PPSK SSID.
			3.	Generate 9999/4096 user accounts for the user group
			4.	Connect 100 clients to the SSID.
			5.	Keep the connection for a long time that exceed the rekey interval. 
			[result 1]
    </description>
    <numofap>1</numofap>
    <numofsta>1</numofsta>
    <tbtype>winlin</tbtype>
    <property name="" value=""/>
    <stage>
                <step no="1">
            <desc>
            1.	Configure a PPSK SSID, bind to wifi0
            </desc>
            <log-file></log-file>
            <telnet target="ap1"></telnet>
            <ssh target="sta1"></ssh>
            <setvar name=""></setvar>
            <script></script>
            <log-match></log-match>
        </step>
        <step no="2">
            <desc>
            2.	Configure a user group, set it to the PPSK SSID.
            </desc>
            <log-file></log-file>
            <telnet target="ap1"></telnet>
            <ssh target="sta1"></ssh>
            <setvar name=""></setvar>
            <script></script>
            <log-match></log-match>
        </step>
        <step no="3">
            <desc>
            3.	Generate 9999/4096 user accounts for the user group
            </desc>
            <log-file></log-file>
            <telnet target="ap1"></telnet>
            <ssh target="sta1"></ssh>
            <setvar name=""></setvar>
            <script></script>
            <log-match></log-match>
        </step>
        <step no="4">
            <desc>
            4.	Connect 100 clients to the SSID.
            </desc>
            <log-file></log-file>
            <telnet target="ap1"></telnet>
            <ssh target="sta1"></ssh>
            <setvar name=""></setvar>
            <script></script>
            <log-match></log-match>
        </step>
        <step no="5">
            <desc>
            5.	Keep the connection for a long time that exceed the rekey interval. 
