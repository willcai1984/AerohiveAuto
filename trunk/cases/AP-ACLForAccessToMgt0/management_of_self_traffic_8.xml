			</desc>
			<log-file>without.netmask.manage.all.3_ping_ap1.log</log-file>
			<ssh host="${tb.server1}" timeout="500">
				-v "ping ${ap1.mgt0.ip} -c 5" ; exit 0
			</ssh>
			<log-match>-e "100% packet loss"</log-match>
		</step>

		<step no="without.netmask.manage.all.4">
			<desc>
