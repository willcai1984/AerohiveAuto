			</telnet>
			<log-match>-c -i "set tcp mss to 1358 sucessfully"</log-match>
			<log-match>-c -i "TCP syn pkt, try to clamp tcp mss to 1358:"</log-match>
			<log-match>-c -i "update mtu to 1398 clamp tcp mss flag: 1"</log-match>
			<no-error-check />
		</step>

