<?xml version="1.0" encoding="UTF-8"?>
<testcase>
    <brief>PADR stage choose primary PPPoE servers</brief>
    <priority>Middle</priority>
    <automated>no</automated>
    <description>
        
		Pre-Condition:

			PPPoE server1&2&3 and the port linked to the Modem is in the same vlan

		Test Procedure:

			1) Open debug switch
			2) Enable PPPoE config on BR
			3) Look up logs
    </description>
    <numofap>1</numofap>
    <numofsta>1</numofsta>
    <tbtype>winlin</tbtype>
    <property name="" value=""/>
    <stage>
                <step no="1">
            <desc>
            
				PPPoE server1&2&3 and the port linked to the Modem is in the same vlan
            </desc>
            <log-file></log-file>
            <telnet target="ap1"></telnet>
            <ssh target="sta1"></ssh>
            <setvar name=""></setvar>
            <script></script>
            <log-match></log-match>
        </step>
        <step no="2">
            <desc>
            1) Open debug switch
            </desc>
            <log-file></log-file>
            <telnet target="ap1"></telnet>
            <ssh target="sta1"></ssh>
            <setvar name=""></setvar>
            <script></script>
            <log-match></log-match>
        </step>
        <step no="3">
            <desc>
            2) Enable PPPoE config on BR
            </desc>
            <log-file></log-file>
            <telnet target="ap1"></telnet>
            <ssh target="sta1"></ssh>
            <setvar name=""></setvar>
            <script></script>
            <log-match></log-match>
        </step>
        <step no="4">
            <desc>
            3) Look up logs
            </desc>
            <log-file></log-file>
            <telnet target="ap1"></telnet>
            <ssh target="sta1"></ssh>
            <setvar name=""></setvar>
            <script></script>
            <log-match></log-match>
        </step>

    </stage>
    <failed-process>
        <step no="1..n">
            .. process
	</step>
    </failed-process>
</testcase>
