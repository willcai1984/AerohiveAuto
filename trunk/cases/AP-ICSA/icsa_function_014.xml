<?xml version="1.0" encoding="UTF-8"?>
<testcase>
	<brief>Check the sequence of the ip-fragmented-packets and to-self-non- management-traffic in FE.</brief>
	<priority>High</priority>
	<automated>yes</automated>
	<description>
		Pre-Condition:

		Test procedure
		1. Operation as case 10, result 1
		2. Operation as case 13, result 2

		Test Procedure:

	</description>
	<numofap>1</numofap>
	<numofsta>2</numofsta>
	<tbtype>linux</tbtype>

	<property name="conn1.ssid" value="${ssid.name}" />
	<property name="conn2.ssid" value="${ssid.name}-2" />

	<stage>
		<step no="1">
			<desc>
				- config hive,ssid info
			</desc>
			<telnet target="ap1" timeout="80">
				-v "security-object icsa"
				-v "security-object icsa default-user-profile-attr 1"
				-v "forwarding-engine drop ip-fragmented-packets"
				-v "ssid ${ssid.name}"
				-v "ssid ${ssid.name} security-object icsa"
				-v "interface wifi0 ssid ${ssid.name}"
				-v "ssid ${ssid.name}-2"
				-v "ssid ${ssid.name}-2 security-object icsa"
				-v "interface wifi0 ssid ${ssid.name}-2"
				-v "interface wifi0 radio channel 1"
				-v "user-profile icsa qos-policy def-user-qos vlan-id 1 attribute 1"
			</telnet>
		</step>

		<include prefix="2" file="subs/sta_wifi_connect_${tb.os}.xml" obj-args="conn1,sta1, ap1" />

		<include prefix="3" file="subs/sta_wifi_connect_${tb.os}.xml" obj-args="conn2,sta2, ap1" />

		<step no="4">
			<desc>setup ip-policy ,mac-policy for ap</desc>
			<log-file>step4_set_ap_ippolicy.log</log-file>
			<telnet target="ap1" timeout="80">
				-v "ip-policy test"
				-v "ip-policy test id 1 action permit"
				-v "user-profile icsa security ip-policy from-access test to-access test"
				-v "mac-policy mactest"
				-v "mac-policy mactest id 1 action permit"
				-v "user-profile icsa security mac-policy from-access mactest to-access mactest"
			</telnet>
		</step>

		<step no="4.1">
			<desc>
				- open debug mode and clear log all
			</desc>
			<telnet target="ap1" timeout="80">
				-v "_kdebug fe detail"
				-v "_kdebug fe basic"
				-v "_ff id 1 src-ip ${mpc.tif.ip} bidirectional"
				-v "clear log all"
				-v "clear forwarding-engine ip-sessions"
			</telnet>
		</step>

		<step no="5">
			<desc>
				mpc send fragment to laptop
			</desc>
			<log-file>step5_mpc_send_fragment.log</log-file>
			<script>
				hping ${sta1.tif.ip} -d 5000 -c 5 -2 &gt; ${log.dir}/${log.file};
				exit 0;
			</script>
			<no-error-check />
		</step>

		<step no="5.1" delay="5" type="loop-check" loop-max-times="5" loop-interval="5">
			<desc>
				show logging buffered an ap1
			</desc>
			<log-file>step5.1_show_logging_buffered_${loop_check.idx}.log</log-file>
			<telnet target="ap1">
				-v "show logging buffered"
			</telnet>
			<no-error-check />
			<log-match>-c -i "${sta1.tif.ip}" -i "${mpc.tif.ip}" -i "drop IP fragment"</log-match>
		</step>

		<step no="6">
			<desc>clear log all</desc>
			<telnet target="ap1" timeout="80">
