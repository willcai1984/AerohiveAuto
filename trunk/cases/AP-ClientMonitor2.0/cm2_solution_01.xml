		<step no="5.7" delay="10">
			<desc>check the STA connection status</desc>
			<log-file>step5.7.log</log-file>
			<ssh target="ap1" timeout="500">
				-v "sh st"
			</ssh>
			<log-match>-c -n -e "${sta2.tif.mac}"</log-match>
		</step>
-->		
		<step no="5.8">
			<desc>clear log</desc>
			<log-file>step5.8.log</log-file>
			<ssh target="ap1">
				-v "cle log all"
			</ssh>
			<no-error-check />
		</step>		
	
	</stage>
	
</testcase>